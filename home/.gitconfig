[user]
	email = chris@chriswlucas.com
	name = Chris Lucas
[color]
        ui = auto
[core]
        pager = less -FMRiX
        excludesfile = ~/.gitignore_global
[push]
        default = simple
[alias]
        dag = log --graph --format='format:%C(yellow)%h%C(reset) %C(blue)\"%an\" <%ae>%C(reset) %C(magenta)%cr%C(reset)%C(auto)%d%C(reset)%n%s' --date-order
[credential]
	helper = cache
